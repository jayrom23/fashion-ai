# OpenAI API Key (required for safety checking)
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Keys (at least one key is required)
# You can add multiple keys for rotation to handle rate limits
GEMINI_API_KEY_01=your_gemini_api_key_here
# GEMINI_API_KEY_02=your_second_gemini_api_key_here
# Add more keys as needed...

# Port Configuration (optional, default is 5002)
# PORT=5002

# Debug mode (set to False in production)
# DEBUG=False

# Allowed origins for CORS (comma-separated)
# ALLOWED_ORIGINS=https://yourdomain.com
